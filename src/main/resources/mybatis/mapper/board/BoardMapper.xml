<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nextinno.doshare.board.mapper.BoardMapper">
    <insert id="addBoard" parameterType="com.nextinno.doshare.domain.boards.Board">
        INSERT INTO BOARD (title, email, uploadDate, content)
        VALUES
        (#{title}, #{email}, #{uploadDate}, #{content})
    </insert>
</mapper>